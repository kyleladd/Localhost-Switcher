{
  "name": "Localhost Switcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Easily switch between localhost and your ip address.",
  "homepage_url": "http://kyleladd.us",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["src/bg/background.js","src/bg/functions.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Localhost Switcher"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "webNavigation",
    "*://*/*"
  ]
}